         <record id="mail_template_data_agent_inquiry" model="mail.template">
          
            <field name="auto_delete" eval="True"/>
            <field name="name">Agent Quotation: Send by Email</field>
            <field name="model_id" ref="model_mlworldwide_freights"/>
            <field name="subject" > {{object.freights_id.agent_inquiry_mail_subject}}</field>
            <!-- <field name="email_from">{{object.user_id.email}}</field> -->
            <field name="email_from">{{object.employee.email}}</field> 
            <field name="email_to"></field>
            <field name="body_html" type="html">
                <t t-set='inquiry' t-value="request.env['freights.agent.inquiry'].search([('id' , '=', object.freights_id.ctx)])"/>
                <t t-if="len(request.env['inquiry.data'].search([('locale','=' ,object.freights_id.customer_id.lang)], limit = 1))>0">
                    <t t-set="k" t-value="request.env['inquiry.data'].search([('locale','=' ,object.freights_id.customer_id.lang)], limit = 1)" />
                </t>
                <t t-else="">
                    <t t-set="k" t-value="request.env['inquiry.data'].search([('locale.code','=','en_US')], limit = 1)" />
                </t>
                <t t-out="k.text_l1"/><br/>
                <!-- <t t-out="object.freights_id.customer_id.name"/> -->
                <t t-out="k.text_l2"/>
                <div style="margin: 0px; padding: 0px;">
                        <p style="margin: 0px; padding: 0px; font-size: 13px;">
                        <br/>

                        <table style="margin-top:20px; width:700px;" class="table" >
                            <tr>
                                <td style="border:  1px solid black;text-align:left ; vertical-align: middle;">
                                    <strong>Reference</strong>
                                    <!-- <span t-esc="inquiry"/> -->
                                    
                                </td>
                                <td style="border:  1px solid black;text-align: left ; vertical-align: middle; ">
                                    <span t-out="object.freights_id.ref_num"/>
                                </td>
                            </tr>
                             <tr>
                                <td style="border:  1px solid black;text-align: left; vertical-align: middle;">
                                    <strong>Mode</strong>
                                </td>
                                <td style="border:  1px solid black;text-align: left; vertical-align: middle;">
                                    <t t-foreach='inquiry.freigths_type' t-as='type'>
                                        <span t-field="type.display_name"/>
                                    </t>
                                </td>
                            </tr>
                            <tr>
                                <td style="border: 1px solid black;text-align: left; vertical-align: middle;">
                                    <strong>Freight Term</strong>
                                </td>
                                <td style="border: 1px solid black;text-align: left; vertical-align: middle;">
                                    <span t-esc="object.freights_id.origin_term.code"/>
                                </td>
                            </tr>
                            <tr>
                                <td style="border: 1px solid black;text-align: left; vertical-align: middle;">
                                    <strong>From</strong>
                                </td>
                                <td style="border: 1px solid black;text-align: left; vertical-align: middle;">
                                    <span t-esc="inquiry.origin_point_id.name"/>
                                </td>
                            </tr>
                            <tr>
                                <td style="border: 1px solid black;text-align: left; vertical-align: middle;">
                                    <strong>To</strong>
                                </td>
                                <td style="border: 1px solid black;text-align: left; vertical-align: middle;">
                                    <span t-esc="inquiry.destination_point_id.name"/>
                                </td>
                            </tr> 
                            <t t-if="inquiry.freigths_type.display_name== 'FCL'">
                                <tr>
                                    <td style="border: 1px solid black;text-align: left; vertical-align: middle;">
                                        <strong>Route</strong>
                                    </td>
                                    <td style="border: 1px solid black;text-align: left; vertical-align: middle;">
                                        <t t-foreach='inquiry.route_id' t-as='route'>
                                            <span t-field="route.display_name"/>
                                        </t>
                                    </td>
                                </tr>

                            </t>
                            
                            <t t-if="object.freights_id.temperature">
                                <tr>
                                    <td style="border: 1px solid black;text-align: left; vertical-align: middle;">
                                        <strong>Temperature Degree</strong>
                                    </td>
                                    <td style="border: 1px solid black;text-align: left; vertical-align: middle;">
                                        <span t-field="object.freights_id.temperature"/>
                                    </td>
                                </tr>
                            </t>
                            <t t-if="object.freights_id.origin_term.code == 'EXW' and object.freights_id.origin_point_id == object.freights_id.origin_point_id ">
                                <tr>
                                    <td style="border: 1px solid black;text-align: left; vertical-align: middle;">
                                        <strong>Pick up address</strong>
                                    </td>
                                    <td style="border: 1px solid black;text-align: left; vertical-align: middle;">
                                        <span t-esc="object.freights_id.pickup_address"/>
                                    </td>
                                </tr>
                            </t>
                            <t t-if="object.freights_id.destination_term.display_name == 'EXW' and object.freights_id.destination_point_id == object.freights_id.destination_point_id ">
                                <tr>
                                    <td style="border: 1px solid black;text-align: left; vertical-align: middle;">
                                        <strong>Delivery address</strong>
                                    </td>
                                    <td style="border: 1px solid black;text-align: left; vertical-align: middle;">
                                        <span t-esc="object.freights_id.delivery_address"/>
                                    </td>
                                </tr>
                            </t>
                            <tr>
                                <td style="border: 1px solid black;text-align: left; vertical-align: middle;">
                                    <strong>Cargo name</strong>
                                </td>
                                <td style="border: 1px solid black;text-align: left; vertical-align: middle;">
                                    <span t-esc="object.freights_id.notes"/>
                                </td>
                            </tr>
                             <t t-if="object.freights_id.hs_code">
                                <tr>
                                    <td style="border: 1px solid black;text-align: left; vertical-align: middle;">
                                        <strong>HS Code</strong>
                                    </td>
                                    <td style="border: 1px solid black;text-align: left; vertical-align: middle;">
                                        <span t-esc="object.freights_id.hs_code"/>
                                    </td>
                                </tr>
                            </t>
                            <t t-if="len(inquiry.freigths_type)>0">
                                <t t-if="inquiry.freigths_type[0].display_name== 'FCL'">
                                    <tr>
                                        <td style="border: 1px solid black;text-align: left; vertical-align: middle;">
                                            <strong>Container type</strong>
                                        </td>
                                        <td style="border: 1px solid black;text-align: left; vertical-align: middle;">
                                            <span>
                                                <t t-out="inquiry.container_type.name"/> /
                                                <t t-foreach="object.freights_id.taras_id" t-as="tara">
                                                    <t t-out="tara.name"/>,
                                                </t>
                                            </span>
                                        </td>
                                    </tr>
                                </t>
                            </t>
                            <tr>
                                <td style="border: 1px solid black;text-align: left; vertical-align: middle;">
                                    <strong>Cargo details</strong>
                                </td>
                                <td style="border: 1px solid black;text-align: left; vertical-align: middle;">
                                    <t t-if="object.freights_id.package_qty">
                                        <span t-esc="object.freights_id.package_qty"/>pcs, 
                                    </t>
                                    <t t-if="object.freights_id.volume">
                                        <span t-esc="object.freights_id.volume"/>cbm, 
                                    </t>
                                     <t t-if="object.freights_id.gross">
                                        <span t-esc="object.freights_id.gross"/>kg, 
                                    </t>
                                </td>
                            </tr>
                            <t t-if="inquiry.freigths_type.display_name == 'FCL'">
                                <tr>
                                    <td style="border: 1px solid black;text-align: left; vertical-align: middle;">
                                        <strong>Shipping line</strong>
                                    </td>
                                    <td style="border: 1px solid black;text-align: left; vertical-align: middle;">
                                       
                                    </td>
                                </tr>
                            </t>
                            <t t-if="inquiry.freigths_type.display_name == 'FTL'">
                                <tr>
                                    <td style="border: 1px solid black;text-align: left; vertical-align: middle;">
                                        <strong>Dimension</strong>
                                    </td>
                                    <td style="border: 1px solid black;text-align: left; vertical-align: middle;">
                                    </td>
                                </tr>
                                <tr>
                                    <td style="border: 1px solid black;text-align: left; vertical-align: middle;">
                                        <strong>Gross weight</strong>
                                    </td>
                                    <td style="border: 1px solid black;text-align: left; vertical-align: middle;">
                                        <span t-field="object.freights_id.gross"/>kg, 
                                    </td>
                                </tr>
                            </t>
                            <tr>
                                <td style="border: 1px solid black;text-align: left; vertical-align: middle;">
                                    <strong>Cargo Status</strong>
                                </td>
                                <td style="border: 1px solid black;text-align: left; vertical-align: middle;">
                                    <t t-if="object.freights_id.dangerous_goods">
                                        <span>Dangerous</span>
                                    </t>
                                    <t t-else="">
                                        <span>Normal</span>
                                    </t>
                                </td>
                            </tr>
                            <tr>
                                <td style="border: 1px solid black;text-align: left;text-color: gray ; vertical-align: middle;">
                                    <strong>Remark</strong>
                                </td>
                                <td style="border: 1px solid black;text-align: left;  vertical-align: middle;">
                                    <span t-esc="object.freights_id.notice"/>
                                </td>
                            </tr> 
                        </table>
                        <br/>
                        <br/>
                        
                        <t t-out="k.text_l3"/><br/>
                        <t t-out="k.text_l4"/><br/>
                        <t t-out="k.text_l5"/><br/>
                        <t t-if="user.signature">
                            <br/>
                            <t t-out="user.signature or ''">
                                <br/>
                            </t>
                        </t>
                    </p>
                </div>
            </field>
            <field name="auto_delete" eval="True"/> 
        </record>
    </data>
    

</odoo>
